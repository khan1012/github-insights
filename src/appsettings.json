{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "GitHubInsights": "Debug"
    }
  },
  "AllowedHosts": "*",
  "GitHub": {
    "Organization": "your-organization-name",
    "Token": "${GITHUB_TOKEN}",
    "CacheDurationMinutes": 5,
    "MaxRepositories": 100,
    "MaxRepositoriesForContributorAnalysis": 20
  }
}
